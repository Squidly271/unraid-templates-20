<?xml version="1.0"?>
<Container version="2">
  <Name>pgAdmin4</Name>
  <Repository>fenglc/pgadmin4</Repository>
  <Beta>False</Beta>
  <Category>Tools:</Category>
  <Date>2017-12-03</Date>
  <Registry>https://hub.docker.com/r/fenglc/pgadmin4/</Registry>
  <Project>https://www.pgadmin.org/</Project>
  <Support>https://lime-technology.com/forum/index.php?topic=TODO</Support>
  <GitHub>https://github.com/rroller/unraid-templates</GitHub>
  <Description>
    PostgreSQL Tools[br][br]
    pgAdmin is the most popular and feature rich Open Source administration and development platform for PostgreSQL, the most advanced Open Source database in the world.
  </Description>
  <Privileged>false</Privileged>
  <Overview/>
  <WebUI>http://[IP]:[PORT:5050]/</WebUI>
  <TemplateURL/>
  <Icon>http://i.imgur.com/N4wDUnJ.png</Icon>
  <ExtraParams/>
  <Config 
    Type="Port" 
    Name="Host Port 1" 
    Target="5050" 
    Default="5050" 
    Mode="tcp" 
    Description="This is pgAdmin4 Web UI Port." 
    Display="always" 
    Required="true">5050</Config>
  <Network>bridge</Network>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>5050</HostPort>
        <ContainerPort>5050</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data/>
  <Environment/>
</Container>